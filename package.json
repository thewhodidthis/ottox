{
  "name": "@thewhodidthis/otto",
  "version": "0.0.6",
  "description": "Helps deal with elementary cellular automata",
  "keywords": [
    "automata",
    "cellular",
    "wolfram"
  ],
  "main": "index.js",
  "module": "index.es",
  "homepage": "https://github.com/thewhodidthis/otto#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/otto.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/otto/issues"
  },
  "directories": {
    "example": "example"
  },
  "config": {
    "remote": "thewhodidthis@zazie:www/example/otto/"
  },
  "scripts": {
    "test": "node test",
    "lint": "eslint . example --ext .es test/*.js",
    "docs": "docco -e .js -l ivory index.es",
    "start": "rollup -c && uglifyjs dist/otto.js -cmo dist/otto.min.js",
    "example": "npm run example:build && npm run example:serve",
    "example:build": "rollup -c -i example/index.es -f iife -o example/index.js",
    "example:serve": "php -S localhost:8000 -t example/",
    "example:press": "rsync -avruzLPC --delete --exclude=.DS_Store ./example/ $npm_package_config_remote"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-plugin-external-helpers": "latest",
    "babel-preset-latest": "latest",
    "docco": "github:67656e65726963/docco",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "tape": "latest",
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
